{
  "type": "panel",
  "background": "#1a1a1a",
  "borderRadius": 12,
  "padding": 20,
  "elements": [
    {
      "type": "image",
      "binding": "dynamicImage",
      "width": "100%",
      "height": 300,
      "objectFit": "cover",
      "borderRadius": 8,
      "marginBottom": 12
    },
    {
      "type": "text",
      "binding": "dynamicTitle",
      "fontSize": 28,
      "color": "#ffffff",
      "align": "center",
      "fontWeight": "bold"
    }
  ]
}






